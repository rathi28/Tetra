<script type="text/javascript">
  $( document ).ready(function() {
      bodyele = $('body');
      bodyele.attr('style', 'background-color:#f5f5f5');
  });
</script>

<% if @pixeltests.empty? == false %>
	<%= render partial: "test_run/pixel_run", locals: {type: params[:suitetype], pixeltests: @pixeltests, test_urls: @test_urls, default_email: @default_email} %>
<% else %>
	<div class="panel panel-default panel-body">
		No <%= params[:suitetype] %> test suites are available to run <br>
		<br>
		<a href="javascript: history.go(-1)" class="btn btn-primary">Go Back</a><br>
		<br>
		<a href="/<%= params[:suitetype] %>/new" class="btn btn-primary">I want to setup a new test run</a><br>
	</div>
<% end %>