<div class="row">
	<div class="col-md-12">
		<div class="panel panel-primary">
			<div class="panel-body" style="overflow: auto;">
				<table class="table table-bordered table-striped table-hover">
					<thead>
						<th>
							Page Type
						</th>
						<th>
							URL
						</th>
						<th>Appender</th>
						<th>MMTest</th>
						<th>Campaign</th>
						<th>Offercode</th>
						<th>UCI</th>
					</thead>
					<% @urls.each_with_index do |url, urlindex| %>
						<tr>
							<td><%= @type[urlindex] ? @type[urlindex] : '' %></td>
							<td><%= url %></td>
							<td><%= @appendstrings[urlindex] ? @appendstrings[urlindex] : '' %></td>
							<td><%= @mmtest[urlindex] ? (@mmtest[urlindex].downcase.include?("y") ? 'yes' : 'no') : 'No' %></td>
							<td><%= @campaign[urlindex] ? @campaign[urlindex] : '' %></td>
							<td><%= @offercode[urlindex] ? @offercode[urlindex] : '' %></td>
							<td><%= @uci[urlindex] ? @uci[urlindex] : '' %></td>
						</tr>
					<% end %>
				</table>
			</div>
		</div>
	</div>
</div>
<br />
<div class="row">
	<div class="col-md-12">
		<div class="panel panel-primary">
			<%= form_tag("/pixels/import", method: "post") do %>
			    <div class="panel-heading">
			        <div class="panel-title">
			        	<label>Import Data from Spreadsheet</label>
			        </div>
			    </div>
			    <div class="panel-body" hidden="true">
			    	<input type="hidden" name="pixelsuite" value="<%= @pixelsuite.id %>">
			        <textarea  name="rawdata" class="form-control col-md-6"><%= params[:rawdata] %></textarea>
			    </div>
			    <br>
			    <div class="panel-footer">
			        <input name="commit" type="submit" value="Finalize Import" class="btn btn-success" />
			    </div>
		    <% end %>
		</div>
	</div>
</div>
