<!-- START OF layouts/application.html.erb -->
<!DOCTYPE html>
<html>
	<head>
		<link href='https://fonts.googleapis.com/css?family=Lato&subset=latin,latin-ext' rel='stylesheet' type='text/css'>
		<title>
			Tetra
		</title>
		<%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
		<%= javascript_include_tag 'application' %>
		<%= csrf_meta_tags %>
		<meta name="viewport" content="width=device-width, initial-scale=1.0">
		<script type="text/javascript">
			<% if @debug_vars %>
				variable_obj = <%= @debug_vars.to_json.html_safe %>
			<% end %>

		</script>
	</head>
	<body>
		<div class="wrapper">
			<%= render template: "layouts/header.html.erb" %>
			<div class="container-fluid">
				<%= render template: "layouts/_error_display.html.erb", locals: {flash: flash} %>
				<%= yield %>
				
				<div class="push">
				</div>
			</div>

		</div>
				<footer style="background-color:#37657B; color:#FFF;" class="footer">
					<div class="container-fluid"> 
						<div class="row">
							<div class="col-md-6">
								<h3 class="brand">Tetra Digital QA Automation</h3>
								<p>QA automation for the testing of Buyflows, Offercode purchasing, UCI codes, and more.</p>
							</div>
						</div>
					</div>
				</footer>

	</body>
</html>
<!-- END OF layouts/application.html.erb -->